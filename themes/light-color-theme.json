{
  "name": "Purply Theme",
  "type": "light",
  "colors": {
    "activityBar.activeBackground": "#3D474D",
    "activityBar.activeBorder": "#8F7EE6",
    "activityBar.background": "#333C40",
    "activityBarBadge.background": "#8F7EE6",
    "badge.background": "#8F7EE6",
    "badge.foreground": "#ffffff",
    "editor.background": "#fcfcfc",
    "editor.selectionBackground": "#8a8a8a56",
    "editor.wordHighlightBackground": "#ececec9f",
    "editorError.foreground": "#e28606",
    "editorGroupHeader.tabsBackground": "#f3f3f3",
    "editorLineNumber.foreground": "#e1e6e7",
    "errorForeground": "#e28606",
    "focusBorder": "#8F7EE6",
    "foreground": "#3D474D",
    "gitDecoration.modifiedResourceForeground": "#de0707",
    "gitDecoration.untrackedResourceForeground": "#76ad3d",
    "list.activeSelectionBackground": "#e7e3f7",
    "list.inactiveSelectionBackground": "#e7e3f7",
    "list.errorForeground": "#e28606",
    "list.activeSelectionForeground": "#3D474D",
    "panelTitle.activeBorder": "#8F7EE6",
    "panelTitle.activeForeground": "#313C41",
    "panelTitle.inactiveForeground": "#848A8C",
    "scrollbarSlider.activeBackground": "#dbdfe1",
    "scrollbarSlider.background": "#ececec",
    "scrollbarSlider.hoverBackground": "#dbdfe1",
    "sideBar.background": "#f3f3f3",
    "sideBarSectionHeader.background": "#8F7EE6",
    "sideBarSectionHeader.border": "#8F7EE6",
    "sideBarSectionHeader.foreground": "#ffffff",
    "statusBar.background": "#262f31",
    "statusBar.debuggingBackground": "#262f31",
    "statusBar.noFolderBackground": "#17191A",
    "tab.activeBackground": "#fcfcfc",
    "tab.activeBorder": "#8F7EE6",
    "tab.border": "#e2e6e7",
    "tab.inactiveBackground": "#e2e6e7",
    "button.background": "#8F7EE6",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#ab9df0",
    "terminal.ansiRed": "#ffe2e2"
  },
  "tokenColors": [
    {
      "name": "Keywords and Storage types",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#fb7f00",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords and Storage types",
      "scope": ["comment.line"],
      "settings": {
        "foreground": "#bbbbbb",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.paragraph.markdown",
        "string.other.link.description.title.markdown"
      ],
      "settings": {
        "foreground": "#3D474D"
      }
    },
    {
      "name": "Quotes",
      "scope": "punctuation.definition.string",
      "settings": {
        "foreground": "#fb7f00"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#5aa9ce"
      }
    },
    {
      "name": "String Quoted",
      "scope": ["string.quoted", "variable.other.readwrite.js"],
      "settings": {
        "fontStyle": "",
        "foreground": "#5aa9ce"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#f3ad4a"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#de0707"
      }
    },
    {
      "name": "JavaScript Other Variable",
      "scope": "variable.other.object.js",
      "settings": {
        "foreground": "#8F7EE6"
      }
    },
    {
      "name": "TypeScript[React] Imports",
      "scope": [
        "variable.other.readwrite.alias.ts",
        "variable.other.readwrite.alias.tsx"
      ],
      "settings": {
        "foreground": "#3D474D"
      }
    },
    {
      "name": "TypeScript[React] Variables and Object Properties",
      "scope": ["variable.object.property.ts", "variable.object.property.tsx"],
      "settings": {
        "foreground": "#de0707"
      }
    },
    {
      "name": "TypeScript[React] Variables and Object Properties",
      "scope": [
        "variable.other.readwrite.ts",
        "variable.other.readwrite.tsx",
        "variable.other.object.ts",
        "variable.other.object.tsx",
        "variable.other.ts",
        "variable.other.tsx",
        "variable.tsx",
        "variable.ts"
      ],
      "settings": {
        "foreground": "#8F7EE6"
      }
    },
    {
      "name": "JavaScript Variable Other ReadWrite",
      "scope": ["variable.other.readwrite.js", "variable.parameter"],
      "settings": {
        "foreground": "#8F7EE6"
      }
    },
    {
      "name": "TypeScript Classes",
      "scope": "meta.class entity.name.type.class.tsx",
      "settings": {
        "foreground": "#de0707"
      }
    },
    {
      "name": "TypeScript Entity Name Type",
      "scope": ["entity.name.type.tsx", "entity.name.type.module.tsx"],
      "settings": {
        "foreground": "#0268c1"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#de0707"
      }
    },
    {
      "name": "Entity Name Type",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#0268c1"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#dc139e"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#8F7EE6"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#8F7EE6"
      }
    },
    {
      "name": "Built-in constant like null",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#a1abb1"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#8F7EE6"
      }
    },
    {
      "name": "Variable Property Other object property",
      "scope": ["variable.other.object.property"],
      "settings": {
        "foreground": "#3D474D"
      }
    },
    {
      "name": "Variable Instances",
      "scope": [
        "variable.instance",
        "variable.other.instance",
        "variable.readwrite.instance",
        "variable.other.readwrite.instance",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#3e3e3e"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#0268c1"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#8F7EE6"
      }
      // #fb7f00
      // #8F7EE6
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#8F7EE6"
      }
    },
    {
      "scope": [
        "markup.italic.markdown",
        "punctuation.definition.italic.markdown"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#303d40"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "meta.var.expr",
        "meta.class meta.method.declaration meta.var.expr storage.type.js",
        "storage.type.property.js",
        "storage.type.property.ts"
      ],
      "settings": {
        "foreground": "#3e3e3e"
      }
    },
    {
      "name": "Keywords operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#67c7de",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "",
        "foreground": "#de0707"
      }
    },
    {
      "scope": [
        "entity.name.section.markdown",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#fb7f00"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#3e3e3e"
      }
    },
    {
      "scope": ["punctuation.definition.metadata.markdown"],
      "settings": {
        "foreground": "#67c7de"
      }
    },
    {
      "scope": [
        "markup.underline.link.image.markdown",
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#de0707"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#de0707"
      }
    },
    {
      "name": "Variables, Let and Const",
      "scope": ["variable.other.readwrites", "meta.definition.variable"],
      "settings": {
        "fontStyle": "",
        "foreground": "#de0707"
      }
    },
    {
      "scope": ["markup.quote.markdown"],
      "settings": {
        "foreground": "#0268c1"
      }
    },
    {
      "scope": ["markup.bold.markdown", "punctuation.definition.bold.markdown"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0268c1"
      }
    },
    {
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#0268c1"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#8F7EE6"
      }
    },
    {
      "name": "TypeScript Method Declaration e.g. `constructor`",
      "scope": [
        "meta.method.declaration storage.type.ts",
        "meta.method.declaration storage.type.tsx"
      ],
      "settings": {
        "foreground": "#8F7EE6"
      }
    },
    {
      "name": "Template Strings",
      "scope": "string.template meta.template.expression",
      "settings": {
        "foreground": "#8F7EE6"
      }
    },
    {
      "name": "Backtics(``) in Template Strings",
      "scope": "string.template punctuation.definition.string",
      "settings": {
        "foreground": "#3e3e3e"
      }
    },
    {
      "name": "Punctuation/Brackets/Tags",
      "scope": ["punctuation.definition.block", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#3D474D"
      }
    },
    {
      "name": "[Markdown] text ",
      "scope": ["meta.paragraph.markdown"],
      "settings": {
        "foreground": "#3e3e3e"
      }
    },
    {
      "name": "Support",
      "scope": [
        "text.html.markdown meta.paragraph meta.link.inline",
        "text.html.markdown meta.paragraph meta.link.inline punctuation.definition.string.begin.markdown",
        "text.html.markdown meta.paragraph meta.link.inline punctuation.definition.string.end.markdown"
      ],
      "settings": {
        "foreground": "#fb7f00"
      }
    },
    {
      "name": "Quotes",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#fb7f00"
      }
    },
    {
      "name": "Link Url",
      "scope": "meta.link",
      "settings": {
        "foreground": "#fb7f00"
      }
    },
    {
      "name": "Dockerfile",
      "scope": "source.dockerfile",
      "settings": {
        "foreground": "#fb7f00"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#8F7EE6"
      }
    },
    {
      "name": "JSON Property values (string)",
      "scope": "meta.structure.dictionary.value.json string.quoted.double",
      "settings": {
        "foreground": "#3e3e3e"
      }
    },
    {
      "name": "JavaScript module imports and exports",
      "scope": [
        "variable.other.meta.import.js",
        "meta.import.js variable.other",
        "variable.other.meta.export.js",
        "meta.export.js variable.other"
      ],
      "settings": {
        "foreground": "#fb7f00"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#de0707"
      }
    },
    {
      "name": "JSON Support Constants",
      "scope": "support.constant.json",
      "settings": {
        "foreground": "#fb7f00"
      }
    },
    {
      "name": "Strings in JSON values",
      "scope": "string.quoted.double.json punctuation.definition.string.json",
      "settings": {
        "foreground": "#3e3e3e"
      }
    },
    {
      "name": "Specific JSON Property values like null",
      "scope": "meta.structure.dictionary.json meta.structure.dictionary.value constant.language",
      "settings": {
        "foreground": "#a1abb1"
      }
    },
    {
      "name": "[JSON] - Support",
      "scope": "source.json support",
      "settings": {
        "foreground": "#fb7f00"
      }
    },
    {
      "name": "[JSON] - String",
      "scope": [
        "source.json string",
        "source.json punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#00820f"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8F7EE6"
      }
    },
    {
      "name": "Meta - Decorator",
      "scope": ["punctuation.decorator"],
      "settings": {
        "foreground": "#8F7EE6"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": " italic bold underline",
        "foreground": "#0268c1"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#0268c1",
        "fontStyle": " bold italic underline"
      }
    },
    {
      "name": "Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#0268c1"
      }
    },
    {
      "name": "Support Variable Property",
      "scope": "support.variable.property",
      "settings": {
        "foreground": "#67c7de"
      }
    },
    {
      "name": "Headings",
      "scope": [
        "markup.heading punctuation.definition.heading",
        "entity.name.section"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#67c7de"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#3D474D"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "foreground": "#3D474D"
      }
    }
  ]
}
